<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="ERROR">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger name="org.eclipse.sensinact.northbound.ws" level="DEBUG"/>
    <logger name="org.eclipse.sensinact.northbound.session" level="DEBUG"/>
</configuration>
