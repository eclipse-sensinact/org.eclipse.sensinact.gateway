  <eClassifiers xsi:type="ecore:EClass" name="ActionParameter" eSuperTypes="http://www.eclipse.org/emf/2002/Ecore#//EParameter #//Timestamped"/>
  <eClassifiers xsi:type="ecore:EClass" name="Timestamped" abstract="true" interface="true">
